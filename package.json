{
  "name": "dok-compression",
  "version": "1.0.28",
  "description": "Made with create-react-library",
  "author": "jacklehamster",
  "license": "MIT",
  "repository": "jacklehamster/dok-compression",
  "type": "module",
  "main": "dist/index.js",
  "module": "src/index.tsx",
  "types": "dist/index.d.ts",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "bun test",
    "build": "bun i && rm -rf *.tsbuildinfo && rm -rf dist/*.* && bun run bundler/bundler.ts && bun run test && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "auto-publish": "npm explore @dobuki/auto-publish -- bun run auto-publish \"$(pwd)\"",
    "build:example": "cd example && bun run build && cd ..",
    "example": "cd example && bun start && cd .."
  },
  "peerDependencies": {
    "react": "^18.0.10"
  },
  "devDependencies": {
    "@dobuki/dev-dependencies": "^1.0.11",
    "@types/blueimp-md5": "^2.18.2"
  },
  "overrides": {
    "eslint": "^8.39.0"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/jacklehamster/dok-compression#readme",
  "dependencies": {
    "blueimp-md5": "^2.19.0",
    "cross-fetch": "^3.1.5",
    "fflate": "^0.7.4",
    "js-yaml": "^4.1.0",
    "stream-data-view": "^1.7.2"
  },
  "keywords": [
    "compression"
  ],
  "publishConfig": {
    "access": "public"
  }
}
